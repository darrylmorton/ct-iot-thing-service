{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es6",
    "strict": true,
    "preserveConstEnums": true,
    "noEmit": false,
    "sourceMap": true,
    "module":"commonjs",
    "moduleResolution":"node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "resolveJsonModule": true
  },
  "exclude": ["node_modules"]
}
